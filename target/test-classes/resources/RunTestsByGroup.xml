<suite name="Group Suite" verbose="1" configfailurepolicy="continue">

    <listeners>
        <listener class-name="customlisteners.Listeners"/>
    </listeners>

    <test name="Grouped Tests" preserve-order="true">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="TestBrowsers"/>
            <class name="ExampleTest"/>
        </classes>
    </test>
</suite>
